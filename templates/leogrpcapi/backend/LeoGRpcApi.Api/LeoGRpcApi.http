@Host = localhost:5200

### Create a new ninja
GRPC {{Host}}/contract.NinjaMgmt/CreateNinja

{
    "rank": "JONIN",
    "code_name": "yami",
    "weapon_proficiencies": [
        "SHURIKEN",
        "STAFF"
    ],
    "special_skills": [
        "walk on water"
    ]
}

> {%
    client.global.set("ninjaId", response.body.id);
%}

### Get ninja by ID
GRPC {{Host}}/contract.NinjaMgmt/GetNinjaById

{
    "id": {{ninjaId}}
}

### Get ninja by _invalid_ ID
GRPC {{Host}}/contract.NinjaMgmt/GetNinjaById

{
    "id": -8
}

### Get all ninja IDs
GRPC {{Host}}/contract.NinjaMgmt/GetAllNinjaIds

### Create mission
GRPC {{Host}}/contract.MissionMgmt/CreateMission

{
    "title": "壁のハエ",
    "description": "Find out the secret behind the exam topic",
    "dangerousness": 0.85
}

> {%
    client.global.set("missionId", response.body.id);
%}

### Get all mission IDs
GRPC {{Host}}/contract.MissionMgmt/GetAllMissions

### Update mission
GRPC {{Host}}/contract.MissionMgmt/UpdateMission

{
    "id": {{missionId}},
    "description": "Top secret mission to find out the secret behind the exam topic",
    "dangerousness": 0.8642
}

### Assign ninja to mission
GRPC {{Host}}/contract.MissionMgmt/AssignMission

{
    "ninja_id": {{ninjaId}},
    "mission_id": {{missionId}}
}

### Delete mission
GRPC {{Host}}/contract.MissionMgmt/DeleteMission

{
    "id": {{missionId}}
}